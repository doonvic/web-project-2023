node {  
    stage('Build') { 
        echo "I am in build stage" 
    }
    stage('Test') { 
        echo "I am in test stage" 
    }
    stage('Deploy') {
        echo "I am in Deploy stage" 
    }
    stage('Archive') { 
        echo "I am in Archive stage"
    }
    stage('Bye-stage') { 
        echo "Job ended. Bye-bye"
    }
}
